{
    "name": "rwoverdijk/assetmanager",
    "description": "An assetmanager module for Zend Framework 3.",
    "license": "BSD-2-Clause",
    "type": "library",
    "keywords": [
        "asset",
        "asset manager",
        "zf3",
        "zend framework"
    ],
    "homepage": "https://github.com/RWOverdijk/AssetManager",
    "authors": [
        {
            "name": "Wesley Overdijk",
            "email": "r.w.overdijk@gmail.com",
            "homepage": "http://blog.spoonx.nl/"
        },
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:wshafer/assetmanager-core.git"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-modulemanager": "^2.7.2",
        "rwoverdijk/assetmanager-core": "^0.0.1",
        "zendframework/zend-psr7bridge": "^0.2.2"
    },
    "require-dev": {
        "zendframework/zend-cache": "^2.7.1",
        "zendframework/zend-mvc-console": "^1.1",
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "~3.0"
    },

    "old-require": {
        "php": "^5.6 || ^7.0",
        "psr/container": "^1.0",
        "zendframework/zend-modulemanager": "^2.7.2",
        "zendframework/zend-stdlib": "^3.1.0",
        "zendframework/zend-loader": "^2.5.1",
        "zendframework/zend-eventmanager": "^3.1.0",
        "zendframework/zend-mvc": "^3.1.0",
        "kriswallsmith/assetic": "^1.4.0"
    },
    "old-require-dev": {
        "zendframework/zend-http": "^2.5.4",
        "zendframework/zend-view": "^2.8.1",
        "zendframework/zend-cache": "^2.7.1",
        "zendframework/zend-console": "^2.6",
        "zendframework/zend-servicemanager": "^3.1.1",
        "zendframework/zend-mvc-console": "^1.1"
    },
    "suggest": {
        "zendframework/zend-mvc-console": "zend-mvc-console provides the ability to expose zend-mvc as a console application"
    },
    "autoload": {
        "psr-4": {
            "AssetManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AssetManager\\Test\\": "tests/"
        }
    }
}
